{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "stocks": [
        "dist/stocks/stocks",
        "dist/stocks"
      ],
      "stocks-mock": [
        "dist/stocks-mock/stocks-mock",
        "dist/stocks-mock"
      ]
    }
  }
}